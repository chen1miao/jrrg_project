AACA;AACA;AACA;AACA;AACA;AACA;AACA,WAAA,KAAA,CAAA,UAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA,EAAA,KAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,gBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AAEA,cAAA,YAAA,CAAA,OAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EAFA,CAEA;AACA;AACA;AACA;AACA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,OAAA;;AACA,cAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,aATA,MASA;AACA,cAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,WAdA;AAeA;AACA,OAlBA;AAmBA,KA5BA;AA6BA;AACA,IAAA,WA9BA,yBA8BA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,QAAA,CAAA,KAAA,aAAA,EAAA,CAAA;AACA,KAjCA;AAkCA;AACA,IAAA,QAnCA,oBAmCA,CAnCA,EAmCA,CAnCA,EAmCA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,aAAA,YAAA,IAAA,KAAA,aAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,KAAA,aAAA,CAAA,MAAA,CAAA,CAAA;AACA;AACA,KAvCA;AAwCA,IAAA,UAxCA,wBAwCA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,KA3CA;AA4CA,IAAA,YA5CA,0BA4CA;AAAA;;AACA,WAAA,OAAA,CAAA,GAAA,CAAA,aAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,yBAAA;;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OAPA;AAQA;AArDA;AA9BA,CAAA","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div style=\"height: 60px; line-height: 60px; font-size: 20px; padding-left: 50px; color: white;\n      background-color: rgba(0,0,0,0.2)\">欢迎使用 GooseGains 量化交易平台</div>\n    <div style=\"margin: 130px 270px; background-color: #fff; width: 400px; height: 300px; padding: 20px; border-radius: 15px\">\n      <div style=\"margin: 20px 0; text-align: center; font-size: 24px\"><b>登 录</b></div>\n      \n      <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\">\n        <el-form-item prop=\"username\">\n          <el-input size=\"medium\" prefix-icon=\"el-icon-user\" v-model=\"user.username\"></el-input>\n        </el-form-item>\n        <el-form-item prop=\"password\">\n          <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" show-password v-model=\"user.password\"></el-input>\n        </el-form-item>\n<!--        <el-form-item>-->\n<!--          <div style=\"display: flex\">-->\n<!--            <el-input size=\"mid\" v-model=\"code\" style=\"width: 200px\"></el-input>-->\n<!--            <span @click=\"refreshCode\" style=\"cursor: pointer; flex: 1;\">-->\n<!--              <Identify :identifyCode=\"identifyCode\"></Identify>-->\n<!--           </span>-->\n<!--          </div>-->\n<!--        </el-form-item>-->\n\n        <el-form-item style=\"margin: 10px 0; text-align: right\">\n          <el-button type=\"warning\" size=\"small\"  autocomplete=\"off\" @click=\"$router.push('/register')\">前往注册</el-button>\n          <el-button type=\"primary\" size=\"small\"  autocomplete=\"off\" @click=\"login\">登录</el-button>\n\n        </el-form-item>\n        <el-form-item style=\"margin: 10px 0; text-align: right\">\n          <el-button type=\"text\" size=\"mid\"  autocomplete=\"off\" @click=\"handlePass\">找回密码</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n\n\n    <el-dialog title=\"找回密码\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\n      <el-form label-width=\"100px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"pass.username\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机号\">\n          <el-input v-model=\"pass.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"passwordBack\">重置密码</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {resetRouter, setRoutes} from \"@/router\";\nimport Identify from \"@/components/Identify\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      user: {},\n      pass: {},\n      code: '',\n      dialogFormVisible: false,\n      // 图片验证码\n      identifyCode: '',\n      // 验证码规则\n      identifyCodes: '3456789ABCDEFGHGKMNPQRSTUVWXY',\n      rules: {\n        username: [\n          { required: true, message: '请输入用户名', trigger: 'blur' },\n          { min: 3, max: 10, message: '长度在 3 到 5 个字符', trigger: 'blur' }\n        ],\n        password: [\n          { required: true, message: '请输入密码', trigger: 'blur' },\n          { min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur' }\n        ],\n      }\n    }\n  },\n  components: {Identify},\n  mounted() {\n    // 重置路由\n    resetRouter()\n    this.refreshCode()\n  },\n  methods: {\n    login() {\n      // if (this.code !== this.identifyCode.toLowerCase()) {\n      //   this.$message({\n      //     type: \"error\",\n      //     message: \"验证码错误\"\n      //   })\n      //   return;\n      // }\n      this.$refs['userForm'].validate((valid) => {\n        if (valid) {  // 表单校验合法\n          this.request.post(\"login\", this.user).then(res => {\n            console.log(res.code)\n            if(res.code === 200) {\n\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // 存储用户信息到浏览器\n              // localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // 存储用户信息到浏览器\n              //\n              // // 动态设置当前用户的路由\n              // setRoutes()\n              this.$router.push(\"/home\")\n              this.$message.success(\"登录成功\")\n            } else {\n              this.$message.error(res.msg)\n            }\n          })\n        }\n      });\n    },\n    // 切换验证码\n    refreshCode() {\n      this.identifyCode = ''\n      this.makeCode(this.identifyCodes, 4)\n    },\n    // 生成随机验证码\n    makeCode(o, l) {\n      for (let i = 0; i < l; i++) {\n        this.identifyCode += this.identifyCodes[Math.floor(Math.random() * (this.identifyCodes.length))]\n      }\n    },\n    handlePass() {\n      this.dialogFormVisible = true\n      this.pass = {}\n    },\n    passwordBack() {\n      this.request.put(\"/user/reset\", this.pass).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"重置密码成功，新密码为：123，请尽快修改密码\")\n          this.dialogFormVisible = false\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style>\n.wrapper {\n  height: 100vh;\n  background-image: linear-gradient(to bottom right, #4169E1 , \t#87CEFA);\n  overflow: hidden;\n}\n</style>\n"],"sourceRoot":"src/views"}]}