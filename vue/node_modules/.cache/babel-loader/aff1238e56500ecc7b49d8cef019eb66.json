AAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAhFA;AAiFA,IAAA,KAjFA,mBAiFA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KApFA;AAqFA,IAAA,gBArFA,4BAqFA,QArFA,EAqFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KAzFA;AA0FA,IAAA,mBA1FA,+BA0FA,OA1FA,EA0FA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA9FA;AA+FA,IAAA,uBA/FA,mCA+FA,GA/FA,EA+FA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KAjGA;AAkGA,IAAA,sBAlGA,kCAkGA,GAlGA,EAkGA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KApGA;AAqGA,IAAA,QArGA,oBAqGA,GArGA,EAqGA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAvGA;AAwGA,IAAA,GAxGA,iBAwGA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,2CAAA;AACA,KA1GA;AA2GA,IAAA,wBA3GA,sCA2GA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA9GA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"email\"></el-input>-->\r\n<!--      <el-input style=\"width: 200px\" placeholder=\"请输入\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>-->\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n      <!-- <el-upload action=\"http://localhost:9090/notification/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button> -->\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"title\" label=\"标题名\"></el-table-column>\r\n      <el-table-column prop=\"publishertime\" label=\"发布时间\"></el-table-column>\r\n      <el-table-column prop=\"publisher\" label=\"作者\"></el-table-column>\r\n      <el-table-column prop=\"views\" label=\"浏览量\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"文章内容\" show-overflow-tooltip=\"true\"></el-table-column>\r\n      <el-table-column prop=\"createBy\" label=\"创建者\"></el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\r\n<!--      <el-table-column label=\"图片\"><template slot-scope=\"scope\"><el-image style=\"width: 100px; height: 100px\" :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image></template></el-table-column>-->\r\n\r\n      <el-table-column label=\"操作\"  width=\"180\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"40%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"140px\" size=\"small\" style=\"width: 85%;\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\r\n        <el-form-item prop=\"title\" label=\"标题名\">\r\n          <el-input v-model=\"form.title\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"publishertime\" label=\"发布时间\">\r\n          <el-date-picker v-model=\"form.publishertime\" type=\"datetime\" value-format=\"yyyy-MM-dd HH:mm:ss\" placeholder=\"选择日期时间\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item prop=\"publisher\" label=\"作者\">\r\n          <el-input v-model=\"form.publisher\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"views\" label=\"浏览量\">\r\n          <el-input v-model=\"form.views\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"content\" label=\"文章内容\">\r\n          <el-input v-model=\"form.content\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"createBy\" label=\"创建者\">\r\n          <el-input v-model=\"form.createBy\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"status\" label=\"状态\">\r\n          <el-input v-model=\"form.status\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"img\" label=\"图片\">\r\n          <el-upload action=\"http://localhost:9090/file/upload\" ref=\"img\" :on-success=\"handleImgUploadSuccess\">\r\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Notification\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      rules: {\r\n        name: [\r\n          { required: true, message: '请输入名称', trigger: 'blur'}\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/notification/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          title: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n        this.$refs['ruleForm'].validate((valid) => {\r\n          if (valid) {\r\n            this.request.post(\"/notification\", this.form).then(res => {\r\n              if (res.code === '200') {\r\n                this.$message.success(\"保存成功\")\r\n                this.dialogFormVisible = false\r\n                this.load()\r\n              } else {\r\n                this.$message.error(res.msg)\r\n              }\r\n            })\r\n          }\r\n        })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/notification/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/notification/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(\"http://localhost:9090/notification/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}