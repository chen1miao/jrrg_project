n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    name: \"FrontHome\",\r\n    data() {\r\n      return {\r\n        lun: [],\r\n        files: [],\r\n        goods: [],\r\n        pageNum: 1,\r\n        pageSize: 20,\r\n        total: 0,\r\n        searchInput:\"\"\r\n      }\r\n    },\r\n    created() {\r\n      this.load()\r\n    },\r\n    methods: {\r\n      load() {\r\n        this.request.get(\"/lun\").then(res => {\r\n          this.lun = res.data\r\n        })\r\n\r\n        this.request.get(\"/dynamics/page\", {\r\n          params: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            name: this.name,\r\n          }\r\n        }).then(res => {\r\n          this.goods = res.data.records\r\n          this.total = res.data.total\r\n        })\r\n      },\r\n      handleSizeChange(pageSize) {\r\n        this.pageSize = pageSize\r\n        this.load()\r\n      },\r\n      handleCurrentChange(pageNum) {\r\n        this.pageNum = pageNum\r\n        this.load()\r\n      },\r\n      searchProduct(){\r\n        this.request.get(\"/dynamics/page\", {\r\n          params: {\r\n            pageNum: this.pageNum,\r\n            pageSize: this.pageSize,\r\n            name: this.searchInput,\r\n          }\r\n        }).then(res => {\r\n          this.goods = res.data.records\r\n          this.total = res.data.total\r\n        })\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\n  .line1 {\r\n    white-space: nowrap;\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n  }\r\n  .el-input--mini .el-input__inner{\r\n    height: 46px;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}