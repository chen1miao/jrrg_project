AAA,QAAA,GAAA,KAAA,IAAA,CAAA,IAAA;AACA,WAAA,QAAA,CAAA,UAAA,GAAA,KAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,WAAA,QAAA,CAAA,SAAA,GAAA,KAAA,MAAA,GAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AAGA,WAAA,OAAA,CAAA,IAAA,CAAA,gBAAA,EAAA,KAAA,QAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,WAAA;;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OAPA;AASA,KApBA;AAqBA,IAAA,UArBA,wBAqBA;AACA;AAEA,WAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,KA1BA;AA2BA,IAAA,aA3BA,yBA2BA,OA3BA,EA2BA;AACA,UAAA,OAAA,KAAA,QAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,UAAA,OAAA,KAAA,MAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA;AAEA;AAnCA;AApBA,CAAA","sourcesContent":["<template>\r\n    <el-container style=\"height: 800px; border: 1px solid #eee\">\r\n  <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\r\n    <el-menu router :default-openeds=\"['1','2','3']\" :default-active=\"this.$router.path\">\r\n     \r\n      <el-submenu index=\"1\">\r\n        <template slot=\"title\"><i class=\"el-icon-message\"></i>个人信息</template>\r\n        <el-menu-item-group>\r\n          <el-menu-item index=\"/changepassword\">修改密码</el-menu-item>\r\n          <el-menu-item index=\"/property\">当前资产</el-menu-item>\r\n       \r\n        </el-menu-item-group>\r\n      </el-submenu>\r\n  \r\n      <el-submenu index=\"2\">\r\n        <template slot=\"title\"><i class=\"el-icon-menu\"></i>市场数据</template>\r\n        <el-menu-item-group>\r\n          \r\n          <el-menu-item index=\"/today\">今日大盘</el-menu-item>\r\n          <el-menu-item index=\"/stock1\">上交所</el-menu-item>\r\n          <el-menu-item index=\"/stock2\">深交所</el-menu-item>\r\n  \r\n        </el-menu-item-group>\r\n        \r\n      </el-submenu>\r\n      <el-submenu index=\"3\">\r\n        <template slot=\"title\"><i class=\"el-icon-setting\"></i>量化交易</template>\r\n        \r\n          <el-menu-item index=\"/trade1\"  class=\"current-page\">买入股票</el-menu-item>\r\n          <el-menu-item index=\"/trade2\">卖出股票</el-menu-item>\r\n        \r\n        <el-submenu index=\"3-2\">\r\n          <template slot=\"title\">交易策略</template>\r\n          <el-menu-item index=\"/strategy1\">策略1</el-menu-item>\r\n          <el-menu-item index=\"/strategy2\">策略2</el-menu-item>\r\n        </el-submenu>\r\n      </el-submenu>\r\n  \r\n    </el-menu>\r\n  </el-aside>\r\n  \r\n  <el-container>\r\n    <el-header style=\"text-align: left; font-size: 16px\">\r\n      <el-dropdown @command=\"handleCommand\">\r\n        <i class=\"el-icon-more\" style=\"margin-right: 20px\"></i>\r\n        <el-dropdown-menu slot=\"dropdown\">\r\n          <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\r\n        </el-dropdown-menu>\r\n      </el-dropdown>\r\n      <span>{{ this.user.user }}</span>\r\n      <span>，欢迎使用 GooseGains 量化交易平台</span>\r\n      \r\n    </el-header>\r\n  \r\n  \r\n    <el-main>\r\n  <div style=\"display: flex; align-items: center;\">\r\n        <div style=\"margin: 150px 0px 110px 260px; background-color: rgb(55, 128, 224,0.2); width: 400px; height: 320px; padding: 20px; border-radius: 15px;\">\r\n            <div style=\"margin: 20px 0; margin-bottom: 30px; text-align: center; font-size: 24px;\"><b>买入股票</b></div>            \r\n            <el-form>\r\n                <el-form-item>\r\n                  <el-button disabled size=\"medium\">当前股票价格：{{ price }}</el-button>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                  <el-button disabled size=\"medium\">计划买入手数：{{ number }}(实际为 {{ number*100 }}支)</el-button>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                  <el-button disabled size=\"medium\">当前该笔消费：{{ number*100*price }}</el-button>\r\n                </el-form-item>\r\n                <el-form-item style=\"margin: 5px 5px; text-align: right\">\r\n                  <el-button @click=\"Click_property()\" type=\"primary\" round>确认交易</el-button> \r\n                  <el-button @click=\"Click_home()\"  type=\"primary\" round>取消交易</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n</div>\r\n</el-main> \r\n  </el-container>\r\n    </el-container>\r\n  </template>\r\n  \r\n  <script>\r\n  export default{\r\n  name:\"Trade11\",\r\n  data() {\r\n    return {\r\n        user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},//当前用户\r\n        price: localStorage.getItem(\"price\") ? JSON.parse(localStorage.getItem(\"price\")) : {},//当前股票价格\r\n        number:localStorage.getItem(\"number\") ? JSON.parse(localStorage.getItem(\"number\")) : {},//当前计划买入手数\r\n        code:localStorage.getItem(\"code\") ? JSON.parse(localStorage.getItem(\"code\")) : {},\r\n        buy_para:{}\r\n    };\r\n    \r\n  },\r\n  mounted() {\r\n    const storedUser = localStorage.getItem(\"user\");\r\n    if (storedUser) {\r\n      // 如果存在存储的用户信息，则解析并更新 user 数据\r\n      this.user = JSON.parse(storedUser);\r\n      console.log(\"用户信息：\", this.user)\r\n    }\r\n  },\r\n  methods: {\r\n      Click_property()//如果点确定交易，\r\n      {\r\n        this.buy_para.id=this.user.id\r\n        this.buy_para.username=this.user.user\r\n        this.buy_para.stock_code=this.code\r\n        this.buy_para.price=this.price\r\n        this.buy_para.amount_in=this.number*100\r\n        console.log(this.buy_para)\r\n        \r\n\r\n        this.request.post(\"transaction_in\", this.buy_para).then(res => {\r\n          if(res.code === 200) {\r\n              this.$router.push(\"/property\")\r\n              this.$message.success(\"买入成功\")\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          })\r\n        \r\n      },\r\n      Click_home()//如果点取消交易，\r\n      {\r\n\r\n        this.$router.push('/trade1')\r\n        this.$message.error(\"交易取消\")\r\n      },\r\n      handleCommand(command) {\r\n        if (command==='logout'){\r\n          this.$router.push('/login');\r\n        }\r\n        if (command==='home'){\r\n          this.$router.push('/home')\r\n        }\r\n          \r\n      }\r\n    }\r\n  }\r\n  </script>\r\n  <style>\r\n  .el-header {\r\n    background-color: #8694a7;\r\n    color: #333;\r\n    line-height: 60px;\r\n  }\r\n  \r\n  .el-aside {\r\n    color: #333;\r\n  }\r\n  </style>"],"sourceRoot":"src/views"}]}